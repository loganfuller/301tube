{
    "webserver": {
        "host": "0.0.0.0",
        "port": 8080,
        "etag": "weak",
        "xPoweredBy": false,
        "compression": {
            "enabled": true,
            "level": 7,
            "threshold": "1kb"
        },
        "static": {
            "etag": true,
            "redirect": true,
            "dotfiles": "ignore",
            "extensions": false,
            "index": false,
            "lastModified": false,
            "maxAge": 0
        }
    },
    "client": {
        "useCdn": false
    },
    "updateCooldownSecs": 60,
    "reddit": {
        "searchCooldownSecs": 80,
        "maxPages": 3,
        "concurrency": 5
    },
    "digg": {
        "searchCooldownSecs": 300,
        "concurrency": 10
    },
    "youTube": {
        "searchCooldownSecs": 70,
        "latestVideoLookbackMins": 45,
        "maxPages": 25,
        "concurrency": 15,
        "apiKey": "REPLACE_WITH_KEY"
    },
    "redis": {
        "uri": null,
        "host": "127.0.0.1",
        "port": 6379,
        "defaultExpirationSeconds": 3600,
        "options": {
            "auth_pass": null
        }
    },
    "db": {
        "uri": "mongodb://localhost/301tube"
    }
}
